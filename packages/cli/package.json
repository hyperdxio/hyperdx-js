{
  "name": "@hyperdx/cli",
  "version": "0.1.0-next.0",
  "description": "HyperDX command line tool",
  "publishConfig": {
    "access": "public"
  },
  "bin": "./dist/index.js",
  "author": "Warren <warren@users.noreply.github.com>",
  "license": "MIT",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./dist/lib": {
      "require": "./dist/lib.cjs",
      "import": "./dist/lib.js",
      "types": "./dist/lib.d.ts"
    }
  },
  "scripts": {
    "typegen": "tsup src/index.ts src/lib.ts --dts-only",
    "clean": "rimraf dist",
    "prebuild": "npm run clean",
    "build": "tsup"
  },
  "installConfig": {
    "hoistingLimits": "workspaces"
  },
  "devDependencies": {
    "@types/node": "^18.15.5",
    "eslint": "^8.39.0",
    "npm-run-all": "^4.1.5",
    "tsup": "6.6.2",
    "typescript": "^5.0.2"
  },
  "dependencies": {
    "commander": "^10.0.0",
    "cross-fetch": "^3.1.5",
    "glob": "^9.3.0"
  }
}
